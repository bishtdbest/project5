.application-form .form-block .no-label label{width: 100%; font-size:14px;}
.application-form .form-block .no-label input[type="checkbox"]{ vertical-align: middle;}
.application-form .form-block  input[type="submit"]{width: 100%;}



.page-content.i-need-page{ padding-left:40px; padding-right:40px; padding-top:100px;}
.need-list{ overflow: hidden;}
.need-list li{width: 16.66%; float: left; padding: 20px; text-align: center;}
.need-list li a{ display: block;}
.need-list li strong{ display: block; padding-top: 50px; font-size: 30px; line-height:1.2; font-weight: normal;}

.need-list li a {color: #1e2731;}
h2.calculator-head{ margin-bottom:0;}
.calculator-form{ background: #fbfbfa; padding:40px; border-radius: 0 0 20px 20px; box-shadow: 0 40px 20px -30px rgba(0,0,0,0.05) }

.emi-calculator-form .form-block{ float: left; width: 50%; padding-right: 30px;}
.emi-calculator-form .form-block li{ padding-bottom: 20px;}
.emi-calculator-form .form-block label{ display: inline-block; width:38%; color: black; font-size:18px;}
.emi-calculator-form .form-block .input-field{ width: 61%;}
.emi-calculator-form .form-block label small{display: block;}

.emi-calculator-buttons { padding: 60px 0 0;}

.emi-calculator-buttons input[type="submit"]{ float: left; width: 30%; }
.emi-calculator-final{ float: left;  width: 70%; padding:0 20px; text-align: center;}
.emi-calculator-final strong{ font-size: 36px; display: block; line-height:1;}
.emi-calculator-final small{ font-size: 18px; display: block;}

.user-eligibility{ padding-top:10px; margin-bottom:0; padding-bottom:0;}
.eligibility-page .left-sidebar{ background:#e9edf4; padding:10px; }

.blue-button-sets{ padding: 10px 0 0 40px;}
.blue-button-sets a.btn{ background:#1a1340; color: white; font-size:14px; max-width: 460px; width: 40%; padding:15px 20px; text-align: center; display: inline-block; margin-right: 20px; border-radius: 10px; }





@media screen and (max-width: 1400px) {
