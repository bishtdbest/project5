.right-sidebar{ width: 20%; float: left;}

.page-content.two-column-right .middle-content,.page-content.two-column-left .middle-content{ width: 80%;}


.left-sidebar ul{ margin-left:0; margin-right:0; padding-left:0;}

.left-sidebar .left-menu{ margin-bottom:50px;}
.left-sidebar .left-menu li{ font-size:16px; color: #1f1f1f; padding:10px; border-bottom: solid 1px #e0e0e0;}
.left-sidebar .left-menu li:last-child{ border-bottom: 0 none;}
.left-sidebar .left-menu li a{ color: #1f1f1f; display: block;}
.left-sidebar .left-menu li a:hover{ color: #df0f00;}

.middle-content p{ color: #1f1f1f; font-size:14px; margin-bottom: 10px; line-height: 1.8;}

.loans-steps-list h3{ text-transform: uppercase; font-size: 18px;	}

.loans-steps-list ul{ padding-right:35px;}

.loans-steps-list li{ float: left; width: 33.33%; position: relative; padding: 25px 0;}
.loans-steps-list.loans-steps-list  li a{ background: #e51d1e; display: block; padding: 15px 50px; font-size: 20px; color: white; line-height: 1; min-height: 70px;}
.loans-steps-list li.blue-bg a{ background: #0a0232 !important;}

.loans-steps-list li a:after{content:""; display: block; width: 78px; height:119px; position: absolute; left:100%; margin-left:-35px; top: 0; z-index:2; background:url(../images/red-arrow.png) no-repeat;}
.loans-steps-list li.blue-bg a:after{background:url(../images/blue-arrow.png) no-repeat;}

.right-sidebar.article-sidebar{ background:#f8f8f8; box-shadow: 1px 1px 0 rgba(233,233,233,0.9); border-radius: 3px;}

.right-sidebar.article-sidebar h4{ padding: 20px; border-bottom: solid 2px white; margin-bottom: 0;}
.article-list{ padding: 20px; margin:0;}
.article-list li{ font-size: 14px; padding:0; padding-bottom: 18px; margin:0;}
.article-list li a{ color: black; }
.article-list li a:hover{ text-decoration: underline;}

.article-sidebar h4:before{content:""; display: block; width: 27px; height:22px;background:url(../images/article-icon.gif) no-repeat; float: left; margin: 0px 8px 0 0;}

.left-sidebar ul.button-sets li{ margin: 24px 0 0;}
.left-sidebar ul.button-sets li a,h2.calculator-head{ display: block; background:#0a0232; color: white; font-size: 16px; position:relative; padding: 10px 10px 10px 40px;}

.left-sidebar ul.button-sets li a:before,h2.calculator-head:before{content:""; display: block; width: 30px; height:100%;background:#ffc900 url(../images/plus-icon.gif) center center no-repeat; background-size: 18px; position: absolute; left:0; top:0; }

.contactbg{background:url(../images/contact-bg.jpg) center center / cover no-repeat; }

.right-slide{ position: absolute; right:0; top:340px; z-index: 10; }

.slide-block{ width: 380px; padding: 25px; background:white; border-radius: 20px; position: absolute; right: -380px;
 -webkit-transition: all 500ms ease;
    -moz-transition: all 500ms ease;
    -o-transition: all 500ms ease;
    transition: all 500ms ease;
	
  -webkit-box-shadow: 5px 5px 5px  rgba(0,0,0,0.5);
     -moz-box-shadow: 5px 5px 5px  rgba(0,0,0,0.5);
          box-shadow: 5px 5px 5px  rgba(0,0,0,0.5);
		  
		   z-index: 20;
}
.slide-block.slide-active{ right: 0px;}

.slide-block li,.slide-block ul{ padding:0; margin:0;}
.slide-block h5{ color: #1a1340; font-size: 24px; text-transform: uppercase; margin:0;}

.slide-block input[type=submit],.slide-block input[type=text],.slide-block input[type=email],select,textarea{ width:100%;}

.slide-block li{padding: 0px 0;}

.slide-block input[type=submit]{background: #e11f04 url(../images/submit-btn.png)  88% no-repeat; }

.slide-tab{position: absolute; width:172px; height:35px;background:#e51d1e; border:0; font-size: 18px; text-align: center; color: white; right:100%; margin-right:-69px; top: 47%; cursor: pointer; text-transform: uppercase;
 /* Safari */
-webkit-transform: rotate(-90deg);

/* Firefox */
-moz-transform: rotate(-90deg);

/* IE */
-ms-transform: rotate(-90deg);

/* Opera */
-o-transform: rotate(-90deg);

-webkit-border-radius: 10px 10px 0px 0px;
-moz-border-radius: 10px 10px 0px 0px;
-ms-border-radius: 10px 10px 0px 0px;
-o-border-radius: 10px 10px 0px 0px;
border-radius: 10px 10px 0px 0px;
}


.feedback.slide-block{ margin-top: 195px; z-index: 15; }
.mobile-app.slide-block{ margin-top: 439px; z-index:12;}

.mobile-app.slide-block div { align: center;}
.mobile-app.slide-block div > img {width:180px;}

.feedback.slide-block .slide-tab{ background:#0a0232 !important;}
.privilege.slide-block .slide-tab{ background-position: -320px top; }
.mobile-app.slide-block .slide-tab{ background-position: -640px top; }

.application-form{ background: #e9edf4; padding:20px;}
