@charset "utf-8";
/* CSS Document */

/* reset css */
html {margin:0;padding:0;border:0;}
body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, code, del, dfn, em, img, q, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, dialog, figure, footer, header, hgroup, nav, section {margin:0;padding:0;border:0;font-size:100%;font:inherit;vertical-align:baseline;}
article, aside, details, figcaption, figure, dialog, footer, header, hgroup, menu, nav, section {display:block;}
body {line-height:1.5;background:white;}
table {border-collapse:separate;border-spacing:0;}
caption, th, td {text-align:left;font-weight:normal;float:none !important;}
table, th, td {vertical-align:middle;}
blockquote:before, blockquote:after, q:before, q:after {content:'';}
blockquote, q {quotes:"" "";}
a img {border:none;}
:focus {outline:0;}

/* typography css */
html {font-size:100.01%;}
body {font-size:75%;color:#222;background:#fff;font-family:'Open Sans', Arial, Helvetica, sans-serif;}
h1, h2, h3, h4, h5, h6 {font-weight:normal;color:#111;}
h1 {font-size:30px;line-height:1;margin-bottom:0.5em;}
h2 {font-size:24px;margin-bottom:30px;}
h3 {font-size:18px;line-height:1;margin-bottom:1em;}
h4 {font-size:16px;line-height:1.25;margin-bottom:1.25em;}
h5 {font-size:18px;font-weight:normal;margin-bottom:1em;}
h6 {font-size:1em;font-weight:bold;}
h1 img, h2 img, h3 img, h4 img, h5 img, h6 img {margin:0;}
p {margin:0 0 1.5em;}
.left {float:left !important;}
p .left {margin:1.5em 1.5em 1.5em 0;padding:0;}
.right {float:right !important;}
p .right {margin:1.5em 0 1.5em 1.5em;padding:0;}
a:focus, a:hover {color:#09f;}
a {color:#06c;text-decoration:none;}
blockquote {margin:1.5em;color:#666;font-style:italic;}
strong, dfn {font-weight:bold;}
em, dfn {font-style:italic;}
sup, sub {line-height:0;}
abbr, acronym {border-bottom:1px dotted #666;}
address {margin:0 0 1.5em;font-style:italic;}
del {color:#666;}
pre {margin:1.5em 0;white-space:pre;}
pre, code, tt {font:1em 'andale mono', 'lucida console', monospace;line-height:1.5;}
li ul, li ol {margin:0;}
/*--ul, ol {margin:0 1.5em 1.5em 0;padding-left:1.5em;}--*/
ul {list-style-type: none;}
ol {list-style-type:decimal;}
dl {margin:0 0 1.5em 0;}
dl dt {font-weight:bold;}
dd {margin-left:1.5em;}
table {margin-bottom:1.4em;width:100%;}
th {font-weight:bold;}
thead th {background:#c3d9ff;}
th, td, caption {padding:4px 10px 4px 5px;}
tbody tr:nth-child(even) td, tbody tr.even td {background:#e5ecf9;}
tfoot {font-style:italic;}
caption {background:#eee;}
.small {font-size:.8em;margin-bottom:1.875em;line-height:1.875em;}
.large {font-size:1.2em;line-height:2.5em;margin-bottom:1.25em;}
.hide {display:none;}
.quiet {color:#666;}
.loud {color:#000;}
.highlight {background:#ff0;}
.added {background:#060;color:#fff;}
.removed {background:#900;color:#fff;}
.first {margin-left:0;padding-left:0;}
.last {margin-right:0;padding-right:0;}
.top {margin-top:0;padding-top:0;}
.bottom {margin-bottom:0;padding-bottom:0;}

/* forms css */
label {font-weight:bold;}
fieldset {padding:0 1.4em 1.4em 1.4em;margin:0 0 1.5em 0;border:1px solid #ccc;}
legend {font-weight:bold;font-size:1.2em;margin-top:-0.2em;margin-bottom:1em;}
fieldset, #IE8#HACK {padding-top:1.4em;}
legend, #IE8#HACK {margin-top:0;margin-bottom:0;}
input{ border: 0 none;}
input[type=text], input[type=password], input[type=url], input[type=email], input.text, input.title, textarea {background-color:#fff;border:1px solid #bbb;color:#000; padding: 10px; min-width: 300px;font-family:'Open Sans', Arial, Helvetica, sans-serif; font-size: 14px;}
input[type=text]:focus, input[type=password]:focus, input[type=url]:focus, input[type=email]:focus, input.text:focus, input.title:focus, textarea:focus {border-color:#666;}
select {background-color:#fff;border-width:1px;border-style:solid;}
input[type=text], input[type=password], input[type=url], input[type=email], input.text, input.title, textarea, select {margin:0.5em 0;}
input.text, input.title {width:300px;padding:5px;}
input.title {font-size:1.5em;}
textarea {height:100px;padding:10px;}
form.inline {line-height:3;}
form.inline p {margin-bottom:0;}
.error, .alert, .notice, .success, .info {padding:0.8em;margin-bottom:1em;border:2px solid #ddd;}
.error, .alert {background:#fbe3e4;color:#8a1f11;border-color:#fbc2c4;}
.notice {background:#fff6bf;color:#514721;border-color:#ffd324;}
.success {background:#e6efc2;color:#264409;border-color:#c6d880;}
.info {background:#d5edf8;color:#205791;border-color:#92cae4;}
.error a, .alert a {color:#8a1f11;}
.notice a {color:#514721;}
.success a {color:#264409;}
.info a {color:#205791;}

select,textarea{background-color:#fff;border:1px solid #bbb;color:#000; padding: 10px; min-width: 300px;font-family:'Open Sans', Arial, Helvetica, sans-serif; font-size: 14px;}

input[type=submit]{background:#e11f04; color: white; font-size:20px; text-transform: uppercase; display: block; text-align: center; padding: 16px 20px; cursor: pointer;}

/* global css */
body{ min-width: 320px;}
#wrapper{max-width:1170px;margin:0 auto; overflow:hidden; min-width: 320px; position: relative;	} 
.container {}
.clearfix:after, .container:after {content:"\0020";display:block;height:0;clear:both;visibility:hidden;overflow:hidden;}
.clearfix, .container {display:block;}
.clear {clear:both;}

*{ box-sizing: border-box;}
img{ max-width: 100%; height:auto;}	

/* header css */

header .container{ padding: 10px 0px ;}

.logo{float:left; width: 20%; min-width: 180px; }
header .navigation{float:right; padding-top: 22px;}
header .navigation ul{ margin-bottom: 0;}
header .navigation li{ float:left; margin-left: 30px;}
header .navigation li a,header .menu{ text-transform: uppercase; font-size: 15px; color: black; cursor: pointer;}
header .navigation li a:hover{ color: #e02626;}


.flexslider .slides li{ position: relative;}
.flexslider .slides h3{ position:absolute; width:50%; padding: 9% 0 0 7%; color: #e0e0e0; z-index:1; text-align:center; left:0; top: 0; }
.flexslider .slides h3 small{ font-size: 28px; font-weight:200; display:block; padding-bottom: 40px;}
.flexslider .slides h3 span{ font-size: 60px;font-weight:400; display:block; line-height: 1.3;}
.flexslider .slides h3 span b{ font-weight:700; display:block;}

.homepage-content{ background:url(../images/content-bg.jpg) no-repeat; text-align: center; padding: 10px 0 10px; background-size: cover;}
.white-block{ width: 80%; margin: 0 auto; background: #f8f8f8; box-shadow: 0 0 15px rgba(0,0,0,0.3); padding: 10px 15px 10px;} 
.white-block em{ font-size: 18px; line-height: 1.8; font-weight: 600; }
.white-block em span{ color: #e02626;}
.white-block strong{ display: block; font-size: 20px; line-height: 1; font-style: italic; padding: 40px 0 60px; }

.white-block .navigation ul{margin-bottom: 0;}
.white-block .navigation li{ float:left; padding-left: 10px; width:20%;}
.white-block .navigation li:first-child{padding-left: 0px;}
.white-block .navigation li a{ background: black; color: white; display:block; line-height: 40px; font-size: 14px;}
.white-block .navigation li a:hover{ background: #e02626;}

.main-content{ border-top: solid 20px #e02626; padding: 15px 10px;  text-align: center; background: #eeeeee;}

.testimonials{ padding-bottom: 20px;}
.testimonials-list{ width: 33.33%; float: left; padding:0 25px;}
.testimonials-block{ background: #f8f8f8; box-shadow: 0 0 15px rgba(0,0,0,0.3); padding: 50px 30px 30px;}
.testimonials-block q::before{ content: url(../images/quote-icon.gif); margin-right: 6px; height:25px; }
.testimonials-block q{ font-size: 18px; line-height: 1.3; min-height: 150px; display:block;}

.testimonials-block .img-wrapper { width: 162px; height:162px; margin: 10px auto; border-radius: 50%; overflow:hidden;}
.testimonials-block em{ font-size: 18px; display:block;}

.partners-list{ padding: 0 0;}
.partners-list img{ margin: 0 20px;}
/* footer css */

footer{ background: #1a1340; }
footer .navigation { padding: 20px 0; border-bottom: solid 1px #616161; margin-bottom: 8px;}
footer .navigation li{ padding: 0 40px; float: left; width: 25%;}
footer .navigation li a{ text-transform: uppercase; font-size: 16px; color: #ffcd00; text-transform: uppercase;}
footer .navigation li a:hover{ color: #fff2be;}
footer .navigation li ul{ padding: 0; margin:0; padding-top:10px;}
footer .navigation li li{float: none; width:100%; padding: 3px 0; margin:0;}
footer .navigation li li a{ color: #eeeeee; font-size: 12px;}

footer .social-icon { padding: 10px 0; border-top: solid 1px #616161;}

.social-icon { text-align: center;}
.social-icon li{ padding: 0 25px; display:inline-block;}
.social-icon li a{ width:30px; height:30px; display:block; background: url(../images/icons-sprite.png) no-repeat;}
.social-icon li a.dribbble-icon{ background-position: 0 -30px;}
.social-icon li a.twitter-icon{ background-position: -68px -30px;}
.social-icon li a.facebook-icon{ background-position: -147px -30px;}
.social-icon li a.instagram-icon{ background-position: -221px -31px;}
.social-icon li a.pinterest-icon{ background-position: -300px -30px;}

.social-icon li a:hover.dribbble-icon{ background-position: 0 2px;}
.social-icon li a:hover.twitter-icon{ background-position: -68px 1px;}
.social-icon li a:hover.facebook-icon{ background-position: -147px 1px;}
.social-icon li a:hover.instagram-icon{ background-position: -221px -2px;}
.social-icon li a:hover.pinterest-icon{ background-position: -300px 1px;}

.login-overlay{ display: none; position:fixed; background: rgba(0,0,0,0.2); height:100%; width:100%; left:0; top:0;z-index: 999;}
.login-wrapper{ display: none; position:fixed; background:white; padding: 20px; box-shadow: 0 0 25px rgba(0, 0, 0, 0.5); left:0; top:0; right:0; bottom:0; min-width:300px; width: 400px; min-height: 400px; height: 50%; margin: auto; z-index: 999;}

.login-wrapper h4 { border-bottom: solid 1px #bbb; padding-bottom: 20px; text-transform: uppercase;}
.login-wrapper li label{display:block; font-size: 14px; clear: both; color:black;}
.login-wrapper li { padding-bottom: 15px;}

.login-wrapper input[type=submit] { padding: 10px 20px;background: black; color: white; display:block; font-size: 19px; cursor: pointer;}

.login-wrapper h4 .close-btn,header .menu{ float:right; width: 40px; height: 40px; display:block; background: url(../images/form-icons.gif) no-repeat; font-size:0; cursor:pointer;}

header .menu{ display: none;}

.login-wrapper h4 .close-btn{ background-position: right top;}

.homepage-service {}
.homepage-service .container{ overflow:hidden; padding: 10px 30px; }
.service-block{ float: left; width: 33.33%;}
.service-block h3{ font-size: 24px; border-bottom: solid 4px #f0f0f3; padding: 0 0 6px 50px; margin-bottom: 25px; font-weight: bold;}
.service-block p{ color: #656b86; font-size: 14px; line-height: 26px; padding: 0 50px;}

.service-block h3 i,.contact-section .contact-item i{ width: 50px; height: 50px; display:inline-block; margin-right: 12px; background: url(../images/icons-sprite.png) left -46px no-repeat; vertical-align:middle; background-size:220px;}
.service-block.products-block h3 i,.contact-section .contact-item.call i{ background-position: -86px -45px;}
.service-block.questions-block h3 i{ background-position:-170px -45px;}

.homepage-app-block{background: url(../images/content-bg2.jpg) right bottom / cover no-repeat; padding: 80px 0 64px 76px; }
.homepage-app-block h3{ color: #1a1340; font-size: 30px; text-transform: uppercase; font-weight: 900; margin-bottom: 55px;}
.homepage-app-block p{ font-size: 20px; color: #1a1340; max-width: 800px; width: 41%; text-shadow: 1px 1px 1px rgba(255,255,255,0.5);}
.button-set { margin-top: 12%; overflow: hidden;}
.button-set a{ margin-right: 40px;}
.button-set a img{ max-width: 220px; vertical-align:top;}


.banner img{ display: block;}
.service-list-wrapper{ text-align: center; background: #eeeeee; border-bottom: solid 30px #e02626;}
.service-list-wrapper h2{ font-size: 36px; padding: 38px 0 50px; margin:0;}
.service-list-wrapper .service-list{ background: white; padding: 15px 0; overflow:hidden; margin:0;}
.service-list a{ display: inline-block; width: 266px;  height: 266px;  background: #1a1340; border-radius:50%; -webkit-border-radius:50%; overflow: hidden; padding-top: 50px; font-size: 24px; line-height: 1.3; color: #ffd325;}
.service-list a strong{ display:block;}
.service-list li{ width: 20%; float: left;}

.service-list a{ width: 180px; height: 180px; padding: 10%;}
	.service-list a img{ max-height: 100px;}
	.service-list a strong{ font-size: 16px;}

.inner-content{ padding-top: 40px; padding-bottom: 50px;}
.inner-content em{ font-size: 24px; font-weight: 900; padding-bottom: 40px; display:block;}
.inner-content .navigation { padding-bottom: 60px;}
.inner-content .navigation li{ display:inline-block; float:none; width: 48%; max-width: 290px; margin: 0 1%; }
.inner-content .faq-wrapper { max-width: 960px; margin: 0 auto; }
.inner-content .faq-wrapper strong{ padding: 0 0 20px; font-weight: normal; font-size: 24px; color: #e11f04; }
.inner-content .faq-wrapper li{ width: 50%; padding: 0 2% 12px; float:left; text-align: left; font-size: 14px; color: #000000; }
.inner-content .faq-wrapper li a{ color: #000000;}

.contact-section { padding: 10px 0 10px 2%}
.contact-section .contact-item { float: left; width: 29%; text-align: center;}
.contact-section .contact-item {}
.contact-section .contact-item strong{ font-size: 18px; font-weight: normal; display:block; padding-top: 25px;}
.contact-section .contact-item i{ background-position:0 -100px;}
.contact-section .contact-item.chat i{ background-position: -86px -100px;}
.contact-section .contact-item a{ color: black;}

.banner{ position: relative;}
.banner-wrapper{ position:absolute; left:0; top: 0; width: 100%; height: 100%;}
.banner-wrapper .banner-content{ padding: 10% 0 0 10%; color: #e0e0e0; font-size: 30px; font-weight: 200;}
.banner-wrapper .banner-content strong{ font-size: 60px; display:block; }

/* homepage css */


/* All Loans css */
.page-heading h1{ background: url(../images/heading-bg.gif) left center repeat-x; text-align: center; font-size: 35px; font-weight: 300; width: 90%; margin: 0 auto 30px; }
.page-heading h1 span{background: white; display: inline-block; padding: 0 50px;}
.breadcrumb { text-align: center;}
.breadcrumb ul{ display: inline-block; padding-bottom: 30px;}
.breadcrumb ul li{ float: left; color: #535353; font-style: italic; padding:0 25px 0 18px; font-size:14px; background: url(../images/breadcrumb-arrow.gif) right center no-repeat;}
.breadcrumb ul li a{color: #535353;}
.breadcrumb ul li:last-child{ background: none;}

.page-content.three-column,.page-content.two-column-right, .page-content.two-column-left{ padding-left:70px; padding-right:70px;}
.page-content{ padding-bottom:10px;}
.left-sidebar{ width: 20%; float: left;}
